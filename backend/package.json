{
  "name": "hcmc-land-subsidence-backend",
  "version": "1.0.0",
  "description": "Backend API cho hệ thống quản lý sụt lún đất TPHCM",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:db": "node src/test-db-connection.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo",
    "seed:analysis": "node scripts/seed-analysis-data.js",
    "seed:many": "node scripts/seed-many-areas.js",
    "seed:many:clear": "node scripts/seed-many-areas.js --clear",
    "seed:data-management": "node scripts/seed-data-management.js",
    "create:data-tables": "node scripts/create-data-management-tables.js",
    "cleanup:imports": "node scripts/cleanup-imports.js"
  },
  "keywords": [
    "land-subsidence",
    "monitoring",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "msnodesqlv8": "^4.1.1",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.73.0",
    "sequelize": "^6.35.0",
    "tedious": "^16.6.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
